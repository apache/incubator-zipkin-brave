import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        String a="";
        for(int i=A.length()-1;i>=0;i--)
        a+=A.charAt(i);
        if(A.compareTo(a)==0)
        System.out.println("Yes");
        else
        System.out.println("No");
    }
}
